# Please replace the placeholder below with your actual Gemini API key.
# Do not commit this file with your actual key to version control.
GEMINI_API_KEY=your_gemini_api_key_here

# Bytez API Configuration
# Replace with your actual Bytez API URL and Key
BYTEZ_API_URL=https://api.bytez.com/v1/run
BYTEZ_API_KEY=your_bytez_api_key_here

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Jitsi JWT-based Authentication Configuration
# These are required for generating secure tokens for "members-only" rooms.
# Get these from your Jitsi installation or 8x8 account.
JITSI_APP_ID=your_jitsi_app_id_here
JITSI_APP_SECRET=your_jitsi_app_secret_here